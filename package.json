{
  "name": "vscode-vimeo-player",
  "displayName": "Vimeo Player",
  "description": "Embed Vimeo videos in a dockable VS Code panel",
  "version": "0.1.0",
  "publisher": "Bombing Around",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "videoPlayer.openVideo",
        "title": "Open Vimeo Video"
      }
    ],
    "views": {
      "panel": [
        {
          "type": "webview",
          "id": "videoPlayer.view",
          "name": "Vimeo Player"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "typescript": "^5.3.0"
  }
}
